#: sslcheck.c:61 sslcheck.c:68
#, c-format
msgid "SSLCHECK %s (%s, %s)\n"
msgstr "SSLCHECK %s (%s, %s)\n"

#: sslcheck.c:71
#, c-format
msgid "sslcheck <domain>     prints domain and remainder of days until cert expires\n"
msgstr "sslcheck <domain>     prints domain and remainder of days until cert expires\n"

#: sslcheck.c:72
#, c-format
msgid "-s --short <domain>   prints only the days\n"
msgstr "-s --short <domain>   prints only the days\n"

#: sslcheck.c:73
#, c-format
msgid "-j --json <domain>    prints output as JSON\n"
msgstr "-j --json <domain>    prints output as JSON\n"

#: sslcheck.c:74
#, c-format
msgid "-h --help             prints this menu\n"
msgstr "-h --help             prints this menu\n"

#: sslcheck.c:75
#, c-format
msgid "-v --version          prints version\n"
msgstr "-v --version          prints version\n"

#: sslcheck.c:133
#, c-format
msgid "Error creating SSL context\n"
msgstr "Error creating SSL context\n"

#: sslcheck.c:145
#, c-format
msgid "Error getting SSL object\n"
msgstr "Error getting SSL object\n"

#: sslcheck.c:155
#, c-format
msgid "Error connecting to %s\n"
msgstr "Error connecting to %s\n"

#: sslcheck.c:163
#, c-format
msgid "No certificate found for %s\n"
msgstr "No certificate found for %s\n"

#: sslcheck.c:176
#, c-format
msgid "Domain: %s | Days until Certification expires: %d\n"
msgstr "Domain: %s | Days until Certification expires: %d\n"

#: sslcheck.c:179
#, c-format
msgid "Could not calculate certificate expiration\n"
msgstr "Could not calculate certificate expiration\n"
