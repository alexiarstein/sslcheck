#: sslcheck.c:33 sslcheck.c:38
#, fuzzy, c-format
msgid "SSLCHECK %s (%s)\n"
msgstr "SSLCHECK %s PIJA\n"

#: sslcheck.c:40
#, c-format
msgid ""
"sslcheck <domain> prints domain and remainder of days until cert expires\n"
msgstr ""
"sslcheck <domaine> affiche le domaine et le nombre de jours restants avant "
"l'expiration du certificat\n"

#: sslcheck.c:41
#, c-format
msgid "sslcheck --short <domain> prints only the days\n"
msgstr "sslcheck --short <domaine> affiche uniquement le nombre de jours\n"

#: sslcheck.c:42
#, c-format
msgid "sslcheck --help prints this menu\n"
msgstr "Ceci est le menu d'aide\n"

#: sslcheck.c:43
#, c-format
msgid "sslcheck --version prints version\n"
msgstr "sslcheck --version affiche la version\n"

#: sslcheck.c:106
#, c-format
msgid "Error creating SSL context\n"
msgstr "Erreur lors de la création du contexte SSL\n"

#: sslcheck.c:118
#, c-format
msgid "Error getting SSL object\n"
msgstr "Erreur lors de la récupération de l'objet SSL\n"

#: sslcheck.c:128
#, c-format
msgid "Error connecting to %s\n"
msgstr "Erreur de connexion à %s\n"

#: sslcheck.c:136
#, c-format
msgid "No certificate found for %s\n"
msgstr "Aucun certificat trouvé pour %s\n"

#: sslcheck.c:148
#, c-format
msgid "Expired %d days ago\n"
msgstr "Expiré il y a %d jours\n"

#: sslcheck.c:154
#, c-format
msgid "Domain: %s | Certificate EXPIRED %d days ago\n"
msgstr "Domaine : %s | Certificat expiré il y a %d jours\n"

#: sslcheck.c:156
#, c-format
msgid "Domain: %s | Days until Certification expires: %d\n"
msgstr "Domaine : %s | Jours restants avant l'expiration du certificat : %d\n"

#: sslcheck.c:160
#, c-format
msgid "Could not calculate certificate expiration\n"
msgstr "Impossible de calculer l'expiration du certificat\n"
